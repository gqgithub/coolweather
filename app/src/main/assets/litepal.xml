<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--dbname配置项目的数据库名称。-->
    <dbname value="cool_weather"></dbname>
    <!--version配置数据库的版本。每次要升级数据库时，加上此处的值-->
    <version value="1"></version>
    <!--list配置映射类。-->
    <list>
        <mapping class="com.gq.coolweather.db.Province"></mapping>
        <mapping class="com.gq.coolweather.db.City"></mapping>
        <mapping class="com.gq.coolweather.db.County"></mapping>
    </list>

</litepal>

